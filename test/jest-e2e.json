{
    "moduleFileExtensions": ["js", "json", "ts"],
    "rootDir": "../",
    "testRegex": ".*\\.e2e-spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": ["**/*.(t|j)s"],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "moduleNameMapper": {
      "^src/(.*)$": "<rootDir>/src/$1"
    },
    "moduleDirectories": ["node_modules", "<rootDir>"]
  }
  